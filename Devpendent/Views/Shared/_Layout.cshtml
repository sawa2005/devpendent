<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Devpendent</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Devpendent.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://use.typekit.net/iuj7zna.css">
    <link rel="apple-touch-icon" sizes="76x76" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#2b5797">
    <meta name="msapplication-TileColor" content="#2b5797">
    <meta name="theme-color" content="#303030">
    <meta name="google-signin-client_id" content="604246437742-m84jvpejnfm5musrf2mrgvvks76cim6c.apps.googleusercontent.com">
</head>
<body>
    <div class="default-main">
        <header>
            <nav class="navbar navbar-expand-sm navbar-toggleable-sm" aria-label="main">
                <div class="nav-menu container-fluid">
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="data:," class="default-logo" alt="Devpendent Logo" /></a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon">
                            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="21" viewBox="0 0 30 21">
                                <g transform="translate(-3 -7.5)">
                                    <path d="M4.5,18h27" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="3" />
                                    <path d="M4.5,9h27" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="3" />
                                    <path d="M4.5,27h27" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="3" />
                                </g>
                            </svg>
                        </span>
                    </button>
                    <div id="navbarSupportedContent" class="navbar-collapse collapse d-sm-inline-flex justify-content-end">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Projects" asp-action="Index">Browse</a>
                            </li>
                            <partial name="_LoginPartial" />
                            <li class="search-wrap">
                                @using (Html.BeginForm("Index", "Projects", FormMethod.Get, new { id = "search-form" }))
                                {
                                    @Html.TextBox("SearchString", "", new { placeholder = "What project do you need done?", @aria_label = "Search Projects" })
                                    <button type="submit" title="Search">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="49" height="48" viewBox="0 0 49 48">
                                            <g stroke="#000" stroke-width="1">
                                                <path d="M0,0H34A15,15,0,0,1,49,15V33A15,15,0,0,1,34,48H0a0,0,0,0,1,0,0V0A0,0,0,0,1,0,0Z" stroke="none" />
                                                <path d="M.5.5H34A14.5,14.5,0,0,1,48.5,15V33A14.5,14.5,0,0,1,34,47.5H.5a0,0,0,0,1,0,0V.5A0,0,0,0,1,.5.5Z" fill="none" />
                                            </g>
                                            <path d="M24.26,23.045,18.7,17.432a7.925,7.925,0,1,0-1.2,1.218l5.525,5.577a.856.856,0,0,0,1.208.031A.861.861,0,0,0,24.26,23.045ZM12.472,18.718A6.258,6.258,0,1,1,16.9,16.886,6.219,6.219,0,0,1,12.472,18.718Z" transform="translate(10.002 9.507)" fill="#fff" />
                                        </svg>
                                    </button>
                                }
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
    </div>
    <div class="container">
        <main>
            <partial name="_NotificationPartial" />
            <div class="row">
                @if (ViewContext.RouteData.Values["area"] == null)
                {
                    <breadcrumb>
                        <li class="breadcrumbs-item home">
                            <a asp-area="" asp-controller="Home" asp-action="Index" title="Home">
                                <svg xmlns="http://www.w3.org/2000/svg" width="14.498" height="15.964" viewBox="0 0 14.498 15.964">
                                    <g id="Icon_feather-home" data-name="Icon feather-home" transform="translate(-3.85 -2.35)">
                                        <path id="Path_5" data-name="Path 5" d="M4.5,8.132,11.1,3l6.6,5.132V16.2a1.466,1.466,0,0,1-1.466,1.466H5.966A1.466,1.466,0,0,1,4.5,16.2Z" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                                        <path id="Path_6" data-name="Path 6" d="M13.5,25.332V18h4.4v7.332" transform="translate(-4.601 -7.668)" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                                    </g>
                                </svg>
                            </a>
                        </li>
                    </breadcrumb>
                }
                @RenderBody()
            </div>
        </main>
    </div>

    <footer class="footer border-footer">
        <div class="footer-main">
            <div>
                <h3>Projects</h3>
                <a asp-controller="Projects" asp-action="Index">All projects</a>
                <vc:categories view="Links" />
            </div>
            <div>
                <h3>About</h3>
                <a asp-controller="About" asp-action="Index">About us</a>
                <a asp-controller="About" asp-action="HowTo">How does Devpendent work?</a>
            </div>
            <div>
                <h3>Terms</h3>
                <a asp-controller="Home" asp-action="Privacy">Privacy policy</a>
                <a asp-controller="Home" asp-action="Accessibility">Accessibility</a>
            </div>
            <div>
                <h3>Contact</h3>
                <a href="mailto:contact@devlancer.one">
                    <svg class="me-1" xmlns="http://www.w3.org/2000/svg" width="16.784" height="13.3" viewBox="0 0 16.784 13.3">
                        <g transform="translate(-2.108 -5.35)">
                            <path d="M4.5,6h12A1.5,1.5,0,0,1,18,7.5v9A1.5,1.5,0,0,1,16.5,18H4.5A1.5,1.5,0,0,1,3,16.5v-9A1.5,1.5,0,0,1,4.5,6Z" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                            <path d="M18,9l-7.5,4.5L3,9" transform="translate(0 -1.5)" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                        </g>
                    </svg>
                    contact@@devpendent.one
                </a>
                <!-- <a href="#">
                    <svg class="me-1" xmlns="http://www.w3.org/2000/svg" width="16.271" height="16.301" viewBox="0 0 16.271 16.301">
                        <path d="M18.138,14.23v2.258A1.505,1.505,0,0,1,16.5,17.994,14.9,14.9,0,0,1,10,15.683a14.678,14.678,0,0,1-4.516-4.516A14.9,14.9,0,0,1,3.174,4.641,1.505,1.505,0,0,1,4.672,3H6.93A1.505,1.505,0,0,1,8.435,4.295,9.665,9.665,0,0,0,8.962,6.41,1.505,1.505,0,0,1,8.623,8l-.956.956a12.043,12.043,0,0,0,4.516,4.516l.956-.956a1.505,1.505,0,0,1,1.588-.339,9.665,9.665,0,0,0,2.115.527,1.505,1.505,0,0,1,1.295,1.528Z" transform="translate(-2.517 -2.35)" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                    </svg>
                    123 456 78 89
                </a> -->
                <a href="https://twitter.com/devpendent" target="_blank">
                    <svg xmlns="http://www.w3.org/2000/svg" width="15.264" height="13.796" viewBox="0 0 15.264 13.796">
                        <path d="M12.021,1.729h2.34L9.248,7.574l6.016,7.952h-4.71L6.865,10.7,2.643,15.526H.3L5.771,9.274,0,1.73H4.83L8.164,6.139Zm-.821,12.4h1.3L4.125,3.057H2.733Z" transform="translate(0 -1.729)" />
                    </svg>
                    X/Twitter
                </a>
                <!-- <a href="#">
                    <svg class="me-1" xmlns="http://www.w3.org/2000/svg" width="16.564" height="16.563" viewBox="0 0 16.564 16.563">
                        <g transform="translate(-2.35 -2.35)">
                            <path d="M6.816,3h7.632a3.816,3.816,0,0,1,3.816,3.816v7.632a3.816,3.816,0,0,1-3.816,3.816H6.816A3.816,3.816,0,0,1,3,14.448V6.816A3.816,3.816,0,0,1,6.816,3Z" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                            <path d="M18.137,14.54a3.053,3.053,0,1,1-2.572-2.572,3.053,3.053,0,0,1,2.572,2.572Z" transform="translate(-4.453 -4.389)" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                            <path d="M26.25,9.75h0" transform="translate(-11.421 -3.316)" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.3" />
                        </g>
                    </svg>
                    Instagram
                </a> -->
            </div>
        </div>
        <div class="text-muted border-footer">
            <p class="copyright">&copy; 2024 - DEVPENDENT - Made by <a href="https://slw.one">Samuel Ward</a></p>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
